<div class="all-posts">
    <div class="post">
        <div class="post-title">
            <a href="/post/{{posts.id}}"><h3>{{posts.title}}</h3></a>
            <h5>Posted by {{posts.user.name}} on {{posts.date_created}}</h5>
        </div>
        <div class="post-content">
            <p>{{posts.content}}</p>
        </div>
    </div>
</div>
